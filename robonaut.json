  "commands": {
    "assemble": [
      {
        "command": "git",
        "args": [
          "clone",
          "{{repository}}",
          "-v"
        ],
        "options": {
          "cwd": "{{robonaut}}"
        }
      },
      {
        "command": "npm",
        "args": [
          "install"
        ],
        "options": {
          "cwd": "{{robonaut}}{{dependency}}"
        }
      }
    ],
    "disassemble": [
      {
        "command": "rm",
        "args": [
          "-rf",
          "{{robonaut}}{{dependency}}"
        ],
        "options": {
          "cwd": "{{robonaut}}"
        }
      }
    ],
    "scan": [
      {
        "command": "git",
        "args": [
          "--no-pager",
          "diff",
          "--word-diff=color",
          "--color=always",
          "-U1",
          "--ws-error-highlight=new,old"
        ],
        "options": {
          "cwd": "{{robonaut}}{{dependency}}"
        }
      }
    ],
    "switch": [
      {
        "command": "git",
        "args": [
          "fetch",
          "--all"
        ],
        "options": {
          "cwd": "{{robonaut}}{{dependency}}"
        }
      },
      {
        "command": "sh",
        "args": [
          "-c",
          "git checkout $(git show-ref --verify --quiet refs/heads/{{branch}} || echo '-b') {{branch}}"
        ],
        "options": {
          "cwd": "{{robonaut}}{{dependency}}"
        }
      }
    ],
    "fuse": {
      "link-out": [
        {
          "command": "npm",
          "args": [
            "link"
          ],
          "options": {
            "cwd": "{{robonaut}}{{dependency}}"
          }
        }
      ],
      "link-in": [
        {
          "command": "npm",
          "args": [
            "link",
            "{{dependency}}"
          ],
          "options": {
            "cwd": "{{robonaut}}{{linkInside}}"
          }
        }
      ]
    },
    "transmit": [
      {
        "git": [
          "add",
          "-A",
          "-v"
        ]
      },
      {
        "git": [
          "commit",
          "-m",
          "\"{{commitMessage}}\"",
          "-v"
        ]
      },
      {
        "git": [
          "push",
          "origin",
          "{{branch}}"
        ]
      },
      {
        "npm": [
          "publish"
        ]
      }
    ]
  }
}
