      "commit": null,
      "version": "1.0.7",
      "repository": "ssh://git@github.com/f1lt3r/markserv-contrib-mod.http-404.git"
    }
  },
  "commands": {
    "assemble": [
      {
        "command": "git",
        "args": [
          "clone",
          "{{repository}}",
          "-v"
        ],
        "options": {
          "cwd": "{{robonaut}}"
        }
      },
      {
        "command": "npm",
        "args": [
          "install"
        ],
        "options": {
          "cwd": "{{robonaut}}{{dependency}}"
        }
      }
    ],
    "disassemble": [
      {
        "command": "rm",
        "args": [
          "-rf",
          "{{robonaut}}{{dependency}}"
        ],
        "options": {
          "cwd": "{{robonaut}}"
        }
      }
    ],
    "scan": [
      {
        "git": [
          "diff",
          "--word-diff=color",
          "--color=always",
          "-U1",
          "--ws-error-highlight=new,old"
        ]
      }
    ],
    "fuse": {
      "link-out": [
        {
          "command": "npm",
          "args": [
            "link"
          ],
          "options": {
            "cwd": "{{robonaut}}{{dependency}}"
          }
        }
      ],
      "link-in": [
        {
          "command": "npm",
          "args": [
            "link",
            "{{dependency}}"
          ],
          "options": {
            "cwd": "{{robonaut}}{{linkInside}}"
          }
        }
      ]
    },
    "transmit": [
      {
        "git": [
          "add",
          "-A",
          "-v"
        ]
      },
      {
        "git": [
          "commit",
          "-m",
          "\"{{commitMessage}}\"",
          "-v"
        ]
      },
      {
        "git": [
          "push",
          "origin",
          "{{branch}}"
        ]
      },
      {
        "npm": [
          "publish"
        ]
      }
    ]
  }
}
