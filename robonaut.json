  "commands": {
    "assemble": [
      {
        "command": "git",
        "args": [
          "clone",
          "-b",
          "{{projectBranch}}",
          "{{repository}}",
          "-v"
        ],
        "options": {
          "cwd": "{{robonaut}}"
        }
      },
      {
        "command": "npm",
        "args": [
          "install"
        ],
        "options": {
          "cwd": "{{robonaut}}{{dependency}}"
        }
      }
    ],
    "disassemble": [
      {
        "command": "rm",
        "args": [
          "-rf",
          "{{robonaut}}{{dependency}}"
        ],
        "options": {
          "cwd": "{{robonaut}}"
        }
      }
    ],
    "scan": [
      {
        "command": "git",
        "args": [
          "status",
          "-s"
        ],
        "options": {
          "cwd": "{{robonaut}}{{dependency}}",
          "stdio": "pipe"
        }
      },
      {
        "command": "git",
        "args": [
          "--no-pager",
          "diff",
          "--word-diff=color",
          "--color=always",
          "-U1",
          "--ws-error-highlight=new,old"
        ],
        "options": {
          "cwd": "{{robonaut}}{{dependency}}",
          "stdio": "pipe"
        }
      }
    ],
    "switch": [
      {
        "command": "git",
        "args": [
          "fetch",
          "--all"
        ],
        "options": {
          "cwd": "{{robonaut}}{{dependency}}"
        }
      },
      {
        "command": "sh",
        "args": [
          "-c",
          "git checkout $(git show-ref --verify --quiet refs/heads/{{branch}} || echo '-b') {{branch}}"
        ],
        "options": {
          "cwd": "{{robonaut}}{{dependency}}"
        }
      }
    ],
    "current": [
      {
        "command": "git",
        "args": [
          "remote",
          "update"
        ],
        "options": {
          "cwd": "{{robonaut}}{{dependency}}"
        }
      },
      {
        "command": "sh",
        "args": [
          "-c",
          "git --no-pager log --ignore-missing HEAD..origin/{{branch}} --oneline --color"
        ],
        "options": {
          "cwd": "{{robonaut}}{{dependency}}",
          "stdio": "pipe"
        }
      }
    ],
    "sync": {
      "git": [
        {
          "command": "git",
          "args": [
            "pull",
            "origin",
            "{{branch}}"
          ],
          "options": {
            "cwd": "{{robonaut}}{{dependency}}"
          }
        }
      ],
      "npm": [
        {
          "command": "git",
          "args": [
            "pull",
            "origin",
            "master"
          ],
          "options": {
            "cwd": "{{robonaut}}{{dependency}}"
          }
        }
      ]
    },
    "test": [
      {
        "command": "npm",
        "args": [
          "test"
        ],
        "options": {
          "cwd": "{{robonaut}}{{dependency}}"
        }
      }
    ],
    "fuse": {
      "link-out": [
        {
          "command": "npm",
          "args": [
            "link"
          ],
          "options": {
            "cwd": "{{robonaut}}{{dependency}}"
          }
        }
      ],
      "link-in": [
        {
          "command": "npm",
          "args": [
            "link",
            "{{dependency}}"
          ],
          "options": {
            "cwd": "{{robonaut}}{{linkInside}}"
          }
        }
      ]
    },
    "transmit": [
      {
      	"command": "git",
      	"args": [
          "add",
          "-A",
          "-v"
        ],
	    "options": {
          "cwd": "{{robonaut}}{{dependency}}"
        }
      },
      {
        "command": "sh",
        "args": [
    	  "-c",
          "git diff --quiet --exit-code --cached || git commit -m \"{{commitMessage}}\" -v"
        ],
	    "options": {
          "cwd": "{{robonaut}}{{dependency}}"
        }
      },
      {
      	"command": "git",
      	"args": [
          "push",
          "origin",
          "{{branch}}"
        ],
	    "options": {
          "cwd": "{{robonaut}}{{dependency}}"
        }
      }
    ],
    "publish": [
      {
      	"command": "git",
      	"args": [
          "add",
          "-A",
          "-v"
        ],
	    "options": {
          "cwd": "{{roboroot}}"
        }
      },
      {
        "command": "sh",
        "args": [
    	  "-c",
          "git diff --quiet --exit-code --cached || git commit -m \"robonaut publish\" -v"
        ],
	    "options": {
          "cwd": "{{roboroot}}"
        }
	  },
	  {
      	"command": "git",
      	"args": [
          "push",
          "origin",
          "{{branch}}"
        ],
	    "options": {
          "cwd": "{{roboroot}}"
        }
      }
    ]
  }
}
